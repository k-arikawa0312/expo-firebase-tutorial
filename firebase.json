{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/account-deletion/**",
        "destination": "/account-deletion/index.html"
      },
      {
        "source": "/privacy-policy/**",
        "destination": "/privacy-policy/index.html"
      }
    ]
  }
}
